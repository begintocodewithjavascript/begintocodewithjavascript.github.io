    "version": "0.2.0",
    "configurations": [
{
    "name": "Attach to Edge",
    "port": 9222,
    "request": "attach",
    "type": "msedge",
    "webRoot": "${workspaceFolder}"
},
{
    "name": "Attach",
    "port": 9229,
    "request": "attach",
    "skipFiles": [
        "<node_internals>/**"
    ],
    "type": "node"
},
{
    "name": "Attach to Chrome",
    "port": 9222,
    "request": "attach",
    "type": "chrome",
    "webRoot": "${workspaceFolder}"
},
        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}